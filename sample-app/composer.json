{
	"name": "tracy-remote-bar/sample-app",
	"description": "Example application for Tracy - remote bars.",
	"require": {
		"php": "^8.0",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.2",
		"nette/di": "^3.1",
		"nette/forms": "^3.1",
		"nette/http": "^3.2",
		"nette/robot-loader": "^4.0",
		"nette/safe-stream": "^3.0",
		"nette/security": "^3.1",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "dev-remote-bars-v2.10.5 as 2.10.5"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse"
	},
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/forrest79/tracy.git"
        }
    ]
}
