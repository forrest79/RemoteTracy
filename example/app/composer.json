{
	"name": "tracy-remote-bar/sample-app",
	"description": "Example application for Tracy - remote bars.",
	"require": {
		"php": "^8.1",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/di": "^3.1",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10"
	},
	"require-dev": {
		"forrest79/tracy-remotedevelopmentstrategy": "@dev",
		"phpstan/phpstan-nette": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse"
	},
	"repositories": [
		{
			"type": "path",
            "url": "../../",
            "options": {
                "symlink": true
            }
		}
	]
}
